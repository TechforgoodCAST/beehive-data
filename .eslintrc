{
    "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb"],
    "plugins": [ "react" ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "globals": {
        "$": true,
        "React": true,
        "ReactDOM": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    }
}
